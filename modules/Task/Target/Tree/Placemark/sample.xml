<Placemark>
	<name>{name}</name>
	<description><![CDATA[<img style="max-width:{maxWidth}px;" src="file:///{src}" title="{title}">]]></description>
	<styleUrl>#{styleId}</styleUrl>
	<Point>
		<gx:drawOrder>1</gx:drawOrder>
		<extrude>1</extrude>
		<altitudeMode>relativeToGround</altitudeMode>
		<coordinates>{longitudeNumber},{latitudeNumber},{height}</coordinates>
	</Point>
</Placemark>
